@model Deployer2._0.Models.ContainerModel

<html>
<head>
    <title> Container Creation Form</title>
</head>
<body style="margin-top: 10px;">
    @using (Html.BeginForm())
    {
    <p>Container Name: @Html.TextBoxFor(x => x.Name)</p>
    <p>Container IP:   @Html.TextBoxFor(x => x.IPAddress)</p>
    <input type="submit"value="Submit Request"/>
    }
</body>
</html>